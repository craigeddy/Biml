<Biml xmlns="http://schemas.varigence.com/biml.xsd">
    <Packages>
        <Package Name="Extract_FF_Character" DelayValidation="true">
			<Annotations>
				<Annotation AnnotationType="CodeComment">
					All flat file extracts are truncate and load.
				</Annotation>
            </Annotations>
			<Tasks>
				<ExecuteSQL Name="EST-Truncate ExtractCharacter" ConnectionName="ADONET_Extract">
					<DirectInput>TRUNCATE TABLE Extract.Character;</DirectInput>
				</ExecuteSQL>
				<Dataflow Name="DFT-Load ExtractCharacter">
					<PrecedenceConstraints>
						<Inputs>
							<Input OutputPathName="EST-Truncate ExtractCharacter.Output" />
                        </Inputs>
                    </PrecedenceConstraints>
					<Transformations>
						<FlatFileSource Name="FF_SRC-Character" ConnectionName="FF_Character" ValidateExternalMetadata="false" />
						<DerivedColumns Name="DER-Audit Columns">
							<InputPath OutputPathName="FF_SRC-Character.Output" />
							<Columns>
								<Column Name="ExecutionID" DataType="AnsiString" Length="50">
									@[System::ExecutionInstanceGUID]
                                </Column>
								<Column Name="InsertedDateTime" DataType="DateTime2">
									GETDATE()
                                </Column>
                            </Columns>
                        </DerivedColumns>
						<OleDbDestination Name="OLE_DEST-ExtractCharacter" ConnectionName="OLEDB_Extract" UseFastLoadIfAvailable="true" ValidateExternalMetadata="false">
							<InputPath OutputPathName="DER-Audit Columns.Output" />
							<ExternalTableOutput Table="Extract.Character" />
							
                        </OleDbDestination>
                    </Transformations>
                </Dataflow>
            </Tasks>
			<Connections>
							<Connection Id="75259484-8563-461e-8cf7-a77987d79ed3" ConnectionName="ADONET_DW" />
							<Connection Id="fdd274e5-c5f9-4b77-989c-21a181473bf3" ConnectionName="ADONET_PDS" />
