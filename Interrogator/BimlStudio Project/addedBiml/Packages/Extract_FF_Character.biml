							<Connection Id="35091f8a-0b30-4a9e-b3ac-8a39aa46afb4" ConnectionName="ADONET_Extract" />
							<Connection Id="66e11f6a-cfb1-4536-adc1-7d95236b16ab" ConnectionName="OLEDB_Transform" />
							<Connection Id="7d62531b-9682-49ad-a229-e87d30d4d4e8" ConnectionName="OLEDB_PDS" />
							<Connection Id="73eba9cd-3f40-449b-9ea7-f9f50cd0a2f5" ConnectionName="OLEDB_Extract" />
							<Connection Id="2bc99379-6810-4471-a3c9-27ad56ae5b94" ConnectionName="OLEDB_DW" /><Connection ConnectionName="FF_Character" />
			</Connections>
